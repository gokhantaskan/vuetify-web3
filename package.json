{
  "name": "vite-vuetify-tailwind",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "npx prettier --write \"**/*.{html,json,md,yml}\"",
    "format:check": "npx prettier --check \"**/*.{html,json,md,yml}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.10.0",
    "@web3-onboard/core": "^2.13.0",
    "@web3-onboard/injected-wallets": "^2.5.0",
    "@web3-onboard/ledger": "^2.4.1",
    "@web3-onboard/vue": "^2.4.4",
    "@web3-onboard/walletconnect": "^2.2.2",
    "lodash": "^4.14.191",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue3-jazzicon": "^0.1.4",
    "vuetify": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@mdi/js": "^7.1.96",
    "@rollup/plugin-inject": "^5.0.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "buffer": "^6.0.3",
    "color.js": "^1.2.0",
    "crypto-browserify": "^3.12.0",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.7.1",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "sass": "^1.56.2",
    "start-server-and-test": "^1.15.2",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-vuetify": "^1.0.1",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
